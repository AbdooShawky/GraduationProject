@model Servicely.Models.RealStateRegistryInterest

@{
    ViewBag.Title = "Delete";
        if (Session["Layout"] != null)
    { Layout = Session["Layout"].ToString(); }

    else
    { Layout = "~/Views/Shared/_Layout.cshtml"; }
}


<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>RealStateRegistryInterest</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            <label>Name RSRI</label>

        </dt>

        <dd>
            @Html.DisplayFor(model => model.realStateRegistryInterest_name)
        </dd>

       

        <dt>
            <label>Place RSRI</label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.District.Region.City.State.state_name)
        </dd>

        <dt>
            <label>Department RSRI</label>

        </dt>

        <dd>
            @Html.DisplayFor(model => model.RealStateRegistryInterestDepartment.realStateRegistryInterestDepartments_name)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-default" /> |
            @Html.ActionLink("Back to List", "Index")
        </div>
    }
</div>

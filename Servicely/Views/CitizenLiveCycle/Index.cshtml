
@{
    ViewBag.Title = "Index";
     if (Session["Layout"] != null)
    { Layout = Session["Layout"].ToString(); }

    else
    { Layout = "~/Views/Shared/_Layout.cshtml"; }
}

<script>
    $(function () {

       
        $("#form").validate()
        // search with date 
        $("#search").click(function () {

            if ($("#form").valid() == true) {
                var from = $("#dd").val();
                var to = $("#date1").val();
                var data = $("#NationalId").val();
                
                $(".tt").empty();

                //citizen info
                
            $.ajax({

                url: "/CitizenLiveCycle/CitizenInfo",
                data: { Id: data },
                async: false,
                success: function (res) {


                    $.each(res, function (i, e) {


                        $("#citizenInfo").append("<tr class='tt'> <td> " + e.firstName + " " + e.secondName + " " + e.thirdName + " " + e.fourthName + "  </td> <td> " + e.birthDate + "   </td> <td> " + e.Age + "  </td><td>  " + e.Relegion + "  </td> <td>  " + e.motherName + " </td>   <td> " + e.gender + "  </td> </tr>");


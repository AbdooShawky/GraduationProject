                        //alert(e.DateTRansactionCitizen);


                    })





                }

            });

            $.ajax({

                url: "/CitizenLiveCycle/getCarLicenceAndViolation",
                data: { Id: data },
                async: false,
                success: function (res) {


                    $.each(res, function (i, e) {


                        $("#carLicence").append("<tr class='tt'> <td>  " + e.carName + " </td> <td> " + e.carModel + " </td> <td> " + e.carCode + " </td>  <td>  " + e.LicenceStartDate + " </td> <td>  " + e.LicenceEndDate + "  </td>     <td> " + e.violationName + "  </td> <td>" + e.violationPrice + " </td><td> " + e.date + "  </td>  </tr>");

                        //$("#violationDate").text(e.date);
                        //alert(e.DateTRansactionCitizen);


                    })





                }

            });

            $.ajax({

                url: "/CitizenLiveCycle/GetAllReservationyBId",
                data: { Id: data },
                async: false,
                success: function (res) {


                    $.each(res, function (i, e) {


                        $("#reservation").append("<tr class='tt'> <td>  " + e.ServiceName + " </td> <td> " + e.ServicePrice + " </td> <td> " + e.OfficeName + " </td>  <td>  " + e.Date + " </td>   </tr>");

                        $("#reservationdate").text(e.TrasactionDate);
                        //alert(e.DateTRansactionCitizen);


                    })





                }

            });

            //-------------------------------------------
            //-------------------------------------------
            //-------------------------------------------
            //--------healthCare Reservation-------------
            $.ajax({

                url: "/CitizenLiveCycle/getReservationHealthCareByCitizenId",
                data: { Id: data },
                async: false,
                success: function (res) {


                    $.each(res, function (i, e) {


                        $("#Health").append("<tr class='tt'> <td> " + e.firstName + " " + e.secondName + " " + e.thirdName + " " + e.fourthName + "  </td> <td> " + e.code + "   </td> <td> " + e.hospitalName + "  </td><td>  " + e.date + "  </td> <td>  " + e.cancel + " </td></tr>");

                        $("#HealthDate").text(e.TransactionDate);
                        //alert(e.DateTRansactionCitizen);


                    })





                }

            });

            //-------------------------------------------
            //-------------------------------------------
            //-------------------------------------------
            //--------Vaccination Reservation-------------
            $.ajax({

                url: "/CitizenLiveCycle/GetAllVaccintionReservationBychId",
                data: { Id: data },
                async: false,
                success: function (res) {


                    $.each(res, function (i, e) {

                        // <td> " + e.firstName + " " + e.secondName + " " + e.thirdName + " " + e.fourthName + "  </td>
                        $("#Vaccination").append("<tr class='tt'>  <td> " + e.code + "   </td><td> " + e.typeName + "  </td> <td> " + e.hospitalName + "  </td><td>  " + e.date + "  </td> <td>  " + e.cancel + " </td></tr>");

                        $("#Vacc").text(e.TransactionDate);
                        //alert(e.DateTRansactionCitizen);


                    })





                }

            });



             $.ajax({

                url: "/CitizenLiveCycle/getUniversityCertificate",
                data: { Id: data },
                async: false,
                success: function (res) {


                    $.each(res, function (i, e) {

                        // <td> " + e.firstName + " " + e.secondName + " " + e.thirdName + " " + e.fourthName + "  </td>
                        $("#educationalUniversity").append("<tr class='tt'>  <td> " + e.universityName + "   </td><td> " + e.facultyName + "  </td> <td> " + e.specializationName + "  </td><td>  " + e.gradeName + "  </td> </tr>");

                        $("#dateGradeFaculty").text(e.date);
                        //alert(e.DateTRansactionCitizen);


                    })





                }

            });


            //---------------------------------------------
            //---------------------------------------------
            //---------------------------------------------
            //-------------------Certificate------------------


                var imge = $('#imge'); 
               imge.attr("src", imge.attr("src") );
                        $("#UName").empty();
                        $("#FName").empty();
                        $("#StudentName").empty();
                $("#FSName").empty();
                        $("#month").empty();
                        $("#Year").empty();
                        $("#Grade").empty();
            $("#date").empty();
     
            
                $.ajax({


                    url: "/CitizenLiveCycle/GetAllDataByStudentId",//GetAllStudentInfoById
                    data: { Id: $("#NationalId").val() },   //, FId: $("#FacultyId").val() },
                    async:false,
                    success: function (res) {


                       

                        $("#UName").text(res.UniversityName);
                        $("#FName").text(res.FacultyName );
                        $("#StudentName").text(res.citizen_first_name + " " + res.citizen_second_name + " " + res.citizen_third_name + " " + res.citizen_fourth_name);
                        $("#FSName").text(res.SpecializationName);
                        var data = res.Date.split("/");
                        $("#month").text(data[1]);
                        $("#Year").text(data[2]);
                        $("#Grade").text(res.GradeName);
                        $("#date").text(res.citizen_birthDate);
                        imge.attr("src", imge.attr("src") + res.UniversityLogo);
                        
                      
                       
                    }

                });

                $("#StateName").empty();
                // state
                   $.ajax({


                    url: "/CitizenLiveCycle/GetAddressByStudentId",
                       data: { Id: $("#NationalId").val() },
                    async:false,
                    success: function (res) {

                        $("#StateName").text(res);
                       
                        
                    }

                   });
                // Photo
                $('#img').empty();
                 var img = $('#img'); 
                 img.attr("src", img.attr("src") );
                   $.ajax({


                    url: "/CitizenLiveCycle/GetPhotoByStudentId",
                    data: { Id: $("#NationalId").val() },
                    async:false,
                    success: function (res) {
                         img.attr("src", img.attr("src") + res.Photo_Url);
                        
                       
                        
                    }

                   });

            $("#btn").show();


        });


    });



</script>

<section class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-3">
                <label>National id</label>
                @Html.DropDownList("NationalId", null, "", new { @class = "form-control" })
            </div>
        </div>
        <div class="container mt-4 ">
            <form id="form">
                <div class="row">
                    <div class="col-md-8" style="border:2px dashed black">
                        <div class="row ">
                            <div class="col-md-5 mt-4">
                                <div class="form-group">
                                    <label>@Servicely.Languages.Language.From</label>
                                    @if (DateTime.Now.Month >= 1 && DateTime.Now.Month < 10)
                                    {
                                        if (DateTime.Now.Day >= 1 && DateTime.Now.Day < 10)
                                        {
                                            <input type="date" name="citizen_birthDate" class="form-control" max="@DateTime.Now.Year-@ViewBag.zero@DateTime.Now.Month-@ViewBag.zero@DateTime.Now.Day" id="dd" required />

                                        }
                                        else
                                        {
                                            <input type="date" name="citizen_birthDate" class="form-control" max="@DateTime.Now.Year-@ViewBag.zero@DateTime.Now.Month-@DateTime.Now.Day" id="dd" required />

                                        }


                                    }
                                    else
                                    {

                                        if (DateTime.Now.Day >= 1 && DateTime.Now.Day < 10)
                                        {
                                            <input type="date"  class="form-control" max="@DateTime.Now.Year-@DateTime.Now.Month-@ViewBag.zero@DateTime.Now.Day" id="dd" required />

                                        }
                                        else
                                        {
                                            <input type="date"  class="form-control" max="@DateTime.Now.Year-@DateTime.Now.Month-@DateTime.Now.Day" id="dd" required />

                                        }

                                    }

                                </div>
                            </div>
                            <div class="col-md-1"></div>
                            <div class="col-md-5 mt-4">
                                <div class="form-group">
                                    <label>@Servicely.Languages.Language.To</label>
                                    @if (DateTime.Now.Month >= 1 && DateTime.Now.Month < 10)
                                    {
                                        if (DateTime.Now.Day >= 1 && DateTime.Now.Day < 10)
                                        {
                                            <input type="date"  class="form-control" max="@DateTime.Now.Year-@ViewBag.zero@DateTime.Now.Month-@ViewBag.zero@DateTime.Now.Day" id="date1" required />

                                        }
                                        else
                                        {
                                            <input type="date"  class="form-control" max="@DateTime.Now.Year-@ViewBag.zero@DateTime.Now.Month-@DateTime.Now.Day" id="date1" required />

                                        }


                                    }
                                    else
                                    {

                                        if (DateTime.Now.Day >= 1 && DateTime.Now.Day < 10)
                                        {
                                            <input type="date"  class="form-control" max="@DateTime.Now.Year-@DateTime.Now.Month-@ViewBag.zero@DateTime.Now.Day" id="date1" required />

                                        }
                                        else
                                        {
                                            <input type="date"  class="form-control" max="@DateTime.Now.Year-@DateTime.Now.Month-@DateTime.Now.Day" id="date1" required />

                                        }

                                    }

                                </div>
                            </div>

                        </div>
                        <div class="row">
                            <div class="col-md-4"></div>
                            <div class="col-md-4"></div>
                            <div class="col-md-4 text-right">
                                <input type="button" id="search" class="btn btn-primary mb-3 mr-3" value="search" />
                            </div>
                        </div>
                    </div>
                    <div class="col-md-4"></div>
                </div>
            </form>
            
            
        </div>
        


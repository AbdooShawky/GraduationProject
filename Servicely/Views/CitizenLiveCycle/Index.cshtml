                        //$("#CurrentAddressDate").text(e.CurrentAddressDate);
                        //alert(e.DateTRansactionCitizen);


                    })





                }

            });


                      //-------------------------------------------
            //-------------------------------------------
            //-------------------------------------------
            //--------healthCare Reservation-------------
            $.ajax({

                url: "/CitizenLiveCycle/getReservationHealthCareByCitizenId2",
                data: { Id: data, from: from, to: to },
                async: false,
                success: function (res) {


                    $.each(res, function (i, e) {


                        $("#Health").append("<tr class='tt'> <td> " + e.firstName + " " + e.secondName + " " + e.thirdName + " " + e.fourthName + "  </td> <td> " + e.code + "   </td> <td> " + e.hospitalName + "  </td><td>  " + e.date + "  </td> <td>  " + e.cancel + " </td></tr>");

                        $("#HealthDate").text(e.TransactionDate);
                        //alert(e.DateTRansactionCitizen);


                    })





                }

            });
               
            }

        });


               $("#btn").hide();
        $("#NationalId").select2();
        $("#NationalId").change(function () {


            var data = $("#NationalId").val();

            // citizen Info
            $(".tt").empty();


            $.ajax({

                url: "/CitizenLiveCycle/CitizenInfo",
                data: { Id: data },
                async: false,
                success: function (res) {


                    $.each(res, function (i, e) {


                        $("#citizenInfo").append("<tr class='tt'> <td> " + e.firstName + " " + e.secondName + " " + e.thirdName + " " + e.fourthName + "  </td> <td> " + e.birthDate + "   </td> <td> " + e.Age + "  </td><td>  " + e.Relegion + "  </td> <td>  " + e.motherName + " </td>   <td> " + e.gender + "  </td> </tr>");


                        $("#CurrentAddressDate").text(e.DateTRansactionCitizen);
                        //alert(e.DateTRansactionCitizen);


                    })


                }

            });
            $.ajax({

                url: "/CitizenLiveCycle/getAddress",
                data: { Id: data },
                async: false,
                success: function (res) {


                    $.each(res, function (i, e) {



                    $.each(res, function (i, e) {


                        $("#SocialStatus").append("<tr class='tt'> <td> " + e.firstName + " " + e.secondName + " " + e.thirdName + " " + e.fourthName + "  </td><td> " + e.wfirstName + " " + e.wsecondName + " " + e.wthirdName + " " + e.wfourthName + "  </td><td> " + e.typeName + "   </td><td>  " + e.from + " </td> <td> " + e.to + " </td><td>  " + e.still + "  </td></tr>");

                        //$("#CurrentAddressDate").text(e.CurrentAddressDate);
                        //alert(e.DateTRansactionCitizen);


                    })





                }

            });



            $.ajax({


@model IEnumerable<Servicely.Models.PoliceReport_type>

@{
    ViewBag.Title = "Index";
       if (Session["Layout"] != null)
    { Layout = Session["Layout"].ToString(); }

    else
    { Layout = "~/Views/Shared/_Layout.cshtml"; }
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            <label>Police Report Type Name</label>
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.police_report_type_name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.police_report_type_id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.police_report_type_id })
        </td>
    </tr>
}

</table>

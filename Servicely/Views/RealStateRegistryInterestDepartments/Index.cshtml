@model IEnumerable<Servicely.Models.RealStateRegistryInterestDepartment>

@{
    ViewBag.Title = "Index";
       if (Session["Layout"] != null)
    { Layout = Session["Layout"].ToString(); }

    else
    { Layout = "~/Views/Shared/_Layout.cshtml"; }
}


<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            <label>RSRI Departments Name</label>
        </th>

       
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.realStateRegistryInterestDepartments_name)
        </td>
      
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.realStateRegistryInterestDepartments_id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.realStateRegistryInterestDepartments_id })
        </td>
    </tr>
}

</table>

@model Servicely.Models.RealStateRegistryInterestPropertyTax

@{
    ViewBag.Title = "Create";
      if (Session["Layout"] != null)
    { Layout = Session["Layout"].ToString(); }

    else
    { Layout = "~/Views/Shared/_Layout.cshtml"; }
}


@using (Html.BeginForm()) 
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">
        <h4>Real State Registry Interest Property Taxes</h4>
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        <div class="form-group">
            <label>RSRI Property Taxes Number</label>
            <div class="col-md-10">
                @Html.EditorFor(model => model.realStateRegistryInterestPropertyTaxes_number, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.realStateRegistryInterestPropertyTaxes_number, "", new { @class = "text-danger" })
            </div>
        </div>

        <div class="form-group">
            <label>RSRI body</label>
            <div class="col-md-10">
                @Html.EditorFor(model => model.realStateRegistryInterestPropertyTaxes_body, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.realStateRegistryInterestPropertyTaxes_body, "", new { @class = "text-danger" })
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </div>
    </div>
}

<div>
